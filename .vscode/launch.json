{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Sale-Here-Backend-service",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "start:debug",
                "--",
                "--inspect-brk",
            ],
            "args": [
                    "${workspaceFolder}/src/main.ts"
                ],
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true,
        },
    ]
}
